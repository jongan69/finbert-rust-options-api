# Alpaca Markets API Credentials
# Get these from: https://app.alpaca.markets/paper/dashboard/overview
APCA_API_KEY_ID=your_alpaca_api_key_here
APCA_API_SECRET_KEY=your_alpaca_secret_key_here

# Optional: Alpaca API Base URL (defaults to paper trading)
APCA_BASE_URL=https://paper-api.alpaca.markets

# Optional: Logging level (debug, info, warn, error)
RUST_LOG=info

# Server Configuration
SERVER_PORT=3000
SERVER_HOST=127.0.0.1

# Model Configuration
SENTIMENT_MODEL_PATH=finbert-onnx
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT_SECS=60
MAX_TEXT_LENGTH=10000
